<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="false">
    <local-check-config name="BmlVCGen" location="config/checkstyle.xml" type="project" description="BmlVCGen checkstyle configuration.">
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <fileset name="Java source" enabled="true" check-config-name="BmlVCGen" local="true">
        <file-match-pattern match-pattern=".java$" include-pattern="true"/>
        <file-match-pattern match-pattern="tests" include-pattern="false"/>
    </fileset>
    <filter name="FilesFromPackage" enabled="true">
        <filter-data value="tests/src"/>
    </filter>
</fileset-config>
