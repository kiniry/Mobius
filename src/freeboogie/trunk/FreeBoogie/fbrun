#!/bin/bash
JFLAGS="-Xss64m -Xms128m -Xmx128m -ea"
if [[ "$OSTYPE" == cygwin ]]; then
  JFLAGS="$JFLAGS -Djline.terminal=jline.UnixTerminal"
fi
SCRIPT=`which "$0"`
HERE=`dirname "$SCRIPT"`
CP="$HERE/classes"
IFS="#"
for lib in "$HERE"/lib/*.jar; do
  CP=$CP:"$lib"
done
if [[ "$OSTYPE" == cygwin ]]; then
  CP=`cygpath -wp $CP`
fi
IFS=" "
#java -verbose:gc -agentlib:hprof=cpu=samples,depth=1 $JFLAGS -cp "$CP" "$@"
java $JFLAGS -cp "$CP" "$@"

